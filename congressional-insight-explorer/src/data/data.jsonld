                    "cx:comments": [
                        {
                            "@type": "http://congress.gov/terms#Comment",
                            "@id": "cx:msg-99",
                            "startTime": "29:46",
                            "text": "That definitely ties in with the MRA budget process issue.",
                            "author": "speaker-5"
